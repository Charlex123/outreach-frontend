
{
	"manifest_version": 3,
	"name": "The Outreach",
	"author": "Outreach",
	"version": "3.0",
	"description": "The Outreach",
	"permissions": ["activeTab" ,"tabs", "storage","scripting"],
	"action": {
	 	"default_title": "The Outreach",
		"default_icon": {
			"16": "images/logo.png"
		},
		"default_popup": "index.html"

	},
	"content_scripts": [
	  {
		"matches": ["https://mail.google.com/*"],
		"js": ["content.js"],
		"css":["stylesrzewb.css"],
		"run_at": "document_end"
	  }
	],
	"background": {
	  "service_worker": "background.js"
	},
	
	"externally_connectable": {
	  "matches": ["https://mail.google.com/*"]
	},
	"host_permissions": ["<all_urls>"],
	"web_accessible_resources": [
		{
		  "resources": [ "stylesrzewb"],
		  "matches": [ "<all_urls>" ]
		},
		{
			"resources": [ "/mailsettings.html"],
			"matches": [ "<all_urls>" ]
		  }
	]
	
  }
  