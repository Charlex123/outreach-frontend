
{
	"manifest_version": 3,
	"name": "The Outreach",
	"author": "Outreach",
	"version": "3.0",
	"description": "The Outreach",
	"permissions": ["activeTab" ,"tabs", "storage","scripting"],
	"action": {
	  "default_icon": {
		"16": "images/logo.png"
	  },
	  "default_popup": "index.html"

	},
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["content.js"],
		"css":["styles.css"],
		"run_at": "document_end"
	  }
	],
	"background": {
	  "service_worker": "background.js"
	},
	
	"externally_connectable": {
	  "matches": ["https://mail.google.com/*"]
	}
  }
  